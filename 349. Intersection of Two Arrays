class Solution(object):
    def intersection(self, nums1, nums2):
        """
        :type nums1: List[int]
        :type nums2: List[int]
        :rtype: List[int]
        """
        
        s1,s2 = set(),set()
        for n in nums1:
            s1.add(n)
            
        for n in nums2:
            if n in s1:
                s2.add(n)
            
       
        return list(s2)
